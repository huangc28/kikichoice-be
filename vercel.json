{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["sfo1", "lhr1", "sin1"],
  "functions": {
    "api/**/*.go": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/hello",
      "destination": "/api/go/entries/hello/core"
    },
    {
      "source": "/v1/webhooks/telegram",
      "destination": "/api/go/entries/telegram/core"
    }
  ]
}